{
  "name": "directus-extension-display-link",
  "description": "Display URLs, phone numbers, and emails with a link button in Directus 9",
  "version": "1.5.1",
  "author": "Jacobo Tabernero Rey <jacoborus@gmail.com>",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-display",
    "link",
    "url",
    "display-link",
    "display-url"
  ],
  "directus:extension": {
    "type": "display",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^9.4.3"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build --watch -o ../extensions/displays/link/index.js",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.4.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vue": "^3.2.28"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacoborus/directus-extension-display-link.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jacoborus/directus-extension-display-link/issues"
  },
  "homepage": "https://github.com/jacoborus/directus-extension-display-link#readme"
}
